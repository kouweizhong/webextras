@{
  ViewBag.Title = "Download";
}
<h3 class="keep-center">Downloads
</h3>
<div class="row">
  <div class="keep-center">
    
    @(Html.Hyperlink(" Stable<small> (v1.2.0)</small>", "http://mihirmone.dyndns.org/app-downloads/webextras/webextras-1.2.0.zip")
          .AddIcon(EGumbyIcon.Download)
          .AsButton(EGumbyButton.Success, EGumbyButtonStyle.Large))

    &nbsp;&nbsp;&nbsp;

    @(Html.Hyperlink(" Nightly", "http://mihirmone.dyndns.org/app-downloads/webextras/webextras-nightly.zip")
      .AddIcon(EGumbyIcon.Download)
      .AsButton(EGumbyButton.Info, EGumbyButtonStyle.Large))

    <br />
    @Html.Hyperlink("Older versions", "javascript:gotoCodeplex()")
  </div>
</div>
<br />
<div class="row">
  <div class="eleven centered columns changelog">
    @Html.Inline(Links.Content.inline.changelog_html)
  </div>
</div>
<br />
<script type="text/javascript">
  $(function () {
    $('.changelog').jScrollPane();
  });
</script>
